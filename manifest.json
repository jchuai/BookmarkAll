{
	"manifest_version": 2,
	"name": "Bookmark All",
	"version": "1.0",
	"description": "Bookmark all tabs to a temp folder.",
	"icons": { "16": "bookmark48.png",
           "48": "bookmark48.png",
          "128": "bookmark128.png" },
	"browser_action": {
		"default_title": "Bookmark All Tabs",
		"default_popup": "popup.html"
	},
	"permissions": [
          "bookmarks",
          "tabs"
        ]
}